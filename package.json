{
  "name": "shift-schedule-manager",
  "version": "1.0.0",
  "description": "React application for managing shift schedules with table editing, export options, and Google Sheets integration",
  "main": "src/index.js",
  "homepage": ".",
  "private": true,
  "keywords": [
    "shift-management",
    "schedule",
    "react",
    "google-sheets",
    "restaurant",
    "japanese"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/shift-schedule-manager.git"
  },
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "dev": "npm start",
    "build": "react-scripts build",
    "build:analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "build:production": "NODE_ENV=production npm run build",
    "test": "react-scripts test",
    "test:watch": "npm test -- --watchAll",
    "test:coverage": "npm test -- --coverage --watchAll=false",
    "test:ci": "npm test -- --coverage --watchAll=false --testResultsProcessor=jest-sonar-reporter",
    "test:performance": "npm test -- --testPathPattern=src/ai/__tests__/.*Performance.*\\.test\\.js --watchAll=false --verbose",
    "test:performance:load": "npm test -- --testPathPattern=src/ai/__tests__/PerformanceLoadTesting\\.test\\.js --watchAll=false",
    "test:performance:integration": "npm test -- --testPathPattern=src/ai/__tests__/MLSystemIntegrationTesting\\.test\\.js --watchAll=false",
    "test:performance:memory": "npm test -- --testPathPattern=src/ai/__tests__/MemoryManagementTesting\\.test\\.js --watchAll=false",
    "test:performance:ux": "npm test -- --testPathPattern=src/ai/__tests__/UserExperienceTesting\\.test\\.js --watchAll=false",
    "test:performance:regression": "npm test -- --testPathPattern=src/ai/__tests__/PerformanceRegressionTesting\\.test\\.js --watchAll=false",
    "test:performance:utils": "npm test -- --testPathPattern=src/ai/__tests__/PerformanceTestUtils\\.test\\.js --watchAll=false",
    "test:performance:suite": "npm test -- --testPathPattern=src/ai/__tests__/MLPerformanceTestSuite\\.test\\.js --watchAll=false --verbose",
    "eject": "react-scripts eject",
    "lint": "eslint src/ --ext .js,.jsx",
    "lint:fix": "npm run lint -- --fix",
    "lint:staged": "lint-staged",
    "format": "prettier --write src/**/*.{js,jsx,json,css,md}",
    "format:check": "prettier --check src/**/*.{js,jsx,json,css,md}",
    "clean": "rm -rf build node_modules/.cache",
    "clean:all": "rm -rf build node_modules package-lock.json && npm install",
    "serve": "npx serve -s build -l 3000",
    "preview": "npm run build && npm run serve",
    "deploy": "npm run build && gh-pages -d build",
    "deploy:netlify": "npm run build && netlify deploy --prod --dir=build",
    "deploy:vercel": "npm run build && vercel --prod",
    "analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "postinstall": "patch-package",
    "prepare": "husky install",
    "validate": "npm run lint && npm run test:coverage",
    "pre-commit": "lint-staged",
    "update-deps": "npm update && npm audit fix",
    "security-check": "npm audit --audit-level=moderate",
    "performance": "lighthouse http://localhost:3000 --output=html --output-path=lighthouse-report.html",
    "docker:build": "docker build -t shift-schedule-manager .",
    "docker:run": "docker run -p 3000:3000 shift-schedule-manager",
    "docker:dev": "docker-compose up -d",
    "docker:stop": "docker-compose down"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/supabase-js": "^2.51.0",
    "@tanstack/react-query": "^5.83.0",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-core": "^4.22.0",
    "@tensorflow/tfjs-layers": "^4.22.0",
    "autoprefixer": "^10.4.14",
    "class-variance-authority": "^0.7.1",
    "classnames": "^2.3.2",
    "clsx": "^2.1.1",
    "date-fns": "^2.30.0",
    "file-saver": "^2.0.5",
    "lodash": "^4.17.21",
    "lucide-react": "^0.263.1",
    "next-themes": "^0.4.6",
    "playwright": "^1.55.0",
    "postcss": "^8.4.27",
    "react": ">=16.8.0",
    "react-day-picker": "^9.9.0",
    "react-dom": ">=16.8.0",
    "react-error-boundary": "^4.1.2",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.62.0",
    "react-hotkeys-hook": "^4.4.1",
    "react-intersection-observer": "^9.5.2",
    "react-scripts": "5.0.1",
    "react-table": "^7.8.0",
    "react-use": "^17.4.0",
    "react-virtualized-auto-sizer": "^1.0.20",
    "react-window": "^1.8.8",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.3.3",
    "uuidv7": "^1.0.2",
    "web-vitals": "^3.3.2",
    "workbox-background-sync": "^7.0.0",
    "workbox-broadcast-update": "^7.0.0",
    "workbox-cacheable-response": "^7.0.0",
    "workbox-core": "^7.0.0",
    "workbox-expiration": "^7.0.0",
    "workbox-google-analytics": "^7.0.0",
    "workbox-navigation-preload": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-range-requests": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0",
    "workbox-streams": "^7.0.0",
    "xlsx": "^0.18.5",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@chromatic-com/storybook": "^1.5.0",
    "@eslint/js": "^9.32.0",
    "@storybook/addon-essentials": "^7.1.0",
    "@storybook/addon-interactions": "^7.1.0",
    "@storybook/addon-links": "^7.1.0",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.1.0",
    "@storybook/react": "^7.1.0",
    "@storybook/react-vite": "^7.1.0",
    "@storybook/testing-library": "^0.2.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.4",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.12",
    "gh-pages": "^5.0.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest-environment-jsdom": "^29.6.1",
    "jest-sonar-reporter": "^2.0.0",
    "lighthouse": "^10.4.0",
    "lint-staged": "^13.2.3",
    "msw": "^1.2.2",
    "msw-storybook-addon": "^1.8.0",
    "netlify-cli": "^15.8.0",
    "patch-package": "^7.0.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "serve": "^14.2.0",
    "shadcn": "^3.0.0",
    "source-map-explorer": "^2.5.3",
    "storybook": "^7.1.0",
    "vercel": "^31.2.3",
    "webpack-bundle-analyzer": "^4.9.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie <= 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/serviceWorker.js",
      "!src/stories/**",
      "!src/**/*.stories.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test:coverage"
    }
  },
  "msw": {
    "workerDirectory": "public"
  }
}
